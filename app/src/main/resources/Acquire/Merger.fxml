<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Acquire.MergerController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="You can now choose to sell, trade, and/or buy stock.">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="88.0" mnemonicParsing="false" onAction="#buyStock" text="Buy Stock">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button layoutX="139.0" layoutY="88.0" mnemonicParsing="false" onAction="#tradeStock" text="Trade Stock">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button layoutX="284.0" layoutY="88.0" mnemonicParsing="false" onAction="#sellStock" text="Sell Stock">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="411.0" layoutY="88.0" mnemonicParsing="false" onAction="#endTurn" text="End Turn">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
